    static async createGeofence(data) {
        const { userId, petId, latitude, longitude, radius, name } = data;
        const center = [longitude, latitude];
        const geofenceArea = {
            center,
            radius: radius || this.config.defaultRadius,
            userId,
            petId,
            name: name || `${petId} çš„åœ°ç†åœæ¬„`,
            isActive: true,
            createdAt: new Date(),
