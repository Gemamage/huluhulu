                    try {
                        // Ê™¢Êü•ÊòØÂê¶Â∑≤ÁôºÈÄÅÈÅéÊ≠§ÊèêÈÜí
                        const existingReminder = await Notification_1.Notification.findOne({
                            userId: pet.userId,
                            type: 'REMINDER',
                            'data.petId': pet._id.toString(),
                            'data.reminderDay': days
                        });
